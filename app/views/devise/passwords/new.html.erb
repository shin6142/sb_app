<div class="main"> 
<div class="container">

<div class="box">
<div class="box-inner">
  <%= form_for(resource, as: resource_name, url: user_session_path) do |f| %>
    <h1>Forgot your password?</h1>
    <div class="box-email">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
    </div>

    <div class="box-login">
    <%= f.submit "Send me reset password instructions" %>
    </div>

  <% end %>

 
</div>
</div>


</div>
</div>












